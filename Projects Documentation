    <script>
        //DocumentsStatusTable

        function DocumentsStatus(ProjectID) {

            var ID = "";

            $.ajax({
                url: '/Dashboard/DocumentsStatus',
                data: { "ProjectID": ProjectID },
                success: function (Data) {

                    for (var I = 0; I <= 7; I++) {
                        for (var J = 0; J <= 2; J++) {
                            //Creating Table Rows IDs

                            switch (JSON.stringify(Data[I][J].diciplineType)) {
                                case JSON.stringify("Process"):
                                    ID = "#Process";
                                    break;

                                case JSON.stringify("Mechanical"):
                                    ID = "#Mechanical";
                                    break;

                                case JSON.stringify("Civil & Arichitecture"):
                                    ID = "#CivilAndEngineering";
                                    break;

                                case JSON.stringify("Electrical"):
                                    ID = "#Electrical";
                                    break;

                                case JSON.stringify("Instrument & Control"):
                                    ID = "#InstrumentAndControl";
                                    break;

                                case JSON.stringify("Piping"):
                                    ID = "#Piping";
                                    break;

                                case JSON.stringify("Quality Control"):
                                    ID = "#QualityControl";
                                    break;

                                case JSON.stringify("All Dicipline"):
                                    ID = "#All";
                                    break;
                            }

                            switch (JSON.stringify(Data[I][J].documentType)) {
                                case JSON.stringify("Basic Design"):
                                    ID = ID + "-BasicDesign";
                                    break;

                                case JSON.stringify("Detail Design"):
                                    ID = ID + "-DetailDesign";
                                    break;

                                case JSON.stringify("Procurement Engineering"):
                                    ID = ID + "-ProcurementEngineering";
                                    break;
                            }

                            var ID1 = ID + "-IssuedByMSV";
                            var ID2 = ID + "-CommentedByFSTCO";
                            var ID3 = ID + "-CommentedByIDOM";
                            var ID4 = ID + "-ApprovedByFSTCO";
                            var ID5 = ID + "-ApprovedByIDOM";
                            var ID6 = ID + "-AppWithNotesByFSTCO";
                            var ID7 = ID + "-AppWithNotesByIDOM";
                            var ID8 = ID + "-NotIssued";
                            var ID9 = ID + "-Delete";
                            var ID10 = ID + "-Total";
                            var ID11 = ID + "-TotalIssued";

                            $(ID1).text(Data[I][J].issuedByMSV);
                            $(ID2).text(Data[I][J].commentedByFSTCO);
                            $(ID3).text(Data[I][J].commentedByIDOM);
                            $(ID4).text(Data[I][J].approvedByFSTCO);
                            $(ID5).text(Data[I][J].approvedByIDOM);
                            $(ID6).text(Data[I][J].appWithNotesByFSTCO);
                            $(ID7).text(Data[I][J].appWithNotesByIDOM);
                            $(ID8).text(Data[I][J].notIssued);
                            $(ID9).text(Data[I][J].delete);
                            $(ID10).text(Data[I][J].total);
                            $(ID11).text(Data[I][J].totalIssued);
                        }
                    }
                }
            });

            TotalDocumentsStatus(ProjectID);
        }

        function TotalDocumentsStatus(ProjectID) {
            $.ajax({
                url: '/Dashboard/TotalDocumentsStatus',
                data: { "ProjectID": ProjectID },
                success: function (Data) {
                    $("#Total-IssuedByMSV").text(Data[0]);
                    $("#Total-CommentedByFSTCO").text(Data[1]);
                    $("#Total-CommentedByIDOM").text(Data[2]);
                    $("#Total-ApprovedByFSTCO").text(Data[3]);
                    $("#Total-ApprovedByIDOM").text(Data[4]);
                    $("#Total-NotIssued").text(Data[5]);
                    $("#Total-Delete").text(Data[6]);
                    $("#Total-AppWithNotesByFSTCO").text(Data[7]);
                    $("#Total-AppWithNotesByIDOM").text(Data[8]);
                    $("#Total-Total").text(Data[7]);
                    $("#Total-TotalIssued").text(Data[8]);
                }
            })
        }

        $('#DocumentsStatusTableProjectID').on('change', function () {
            DocumentsStatus($('#DocumentsStatusTableProjectID').val());
        });

    </script>

    <script>

        $('#AllProjectID').on('change', function () {
            $('#DiciplineProduceProcessProjectID').val($("#AllProjectID option:selected").val()).change();
            $('#AllDocumentsProjectID').val($("#AllProjectID option:selected").val()).change();
            $('#AFCDocumentsProjectID').val($("#AllProjectID option:selected").val()).change();
            $('#AverageAFCDocumentsProjectID').val($("#AllProjectID option:selected").val()).change();
            $('#AverageToGetAFCProjectID').val($("#AllProjectID option:selected").val()).change();
            $('#DiciplineProduceProcessProjectID').val($("#AllProjectID option:selected").val()).change();
            $('#DocumentsStatusTableProjectID').val($("#AllProjectID option:selected").val()).change();
        });

        $('#AllYear').on('change', function () {

            $('#TransmitalDateStart').val($("#AllYear option:selected").val().replace(/\d/g, d => '۰۱۲۳۴۵۶۷۸۹'[d]) + "/۰۱/۰۱").change();
            $('#TransmitalDateEnd').val($("#AllYear option:selected").val().replace(/\d/g, d => '۰۱۲۳۴۵۶۷۸۹'[d]) + "/۱۲/۲۹").change();

            $('#DiciplineProduceProcessYear').val($("#AllYear option:selected").val()).change();
            $('#AllDocumentsYear').val($("#AllYear option:selected").val()).change();
            $('#AFCDocumentsYear').val($("#AllYear option:selected").val()).change();
            $('#AverageAFCDocumentsYear').val($("#AllYear option:selected").val()).change();
            $('#AverageToGetAFCYear').val($("#AllYear option:selected").val()).change();
            $('#DiciplineProduceProcessYear').val($("#AllYear option:selected").val()).change();
        });
    </script>
