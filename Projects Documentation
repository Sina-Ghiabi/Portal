        function AllDocumentsFunction(ProjectID, SelectedYear, SelectedMonth) {
            $.ajax({
                url: '/Dashboard/AllDocuments',
                data: { "ProjectID": ProjectID, "Year": SelectedYear, "Month": SelectedMonth },
                success: function (data) {
                    var DiciplineTypes = [];
                    var DiciplineValues = [];
                    AllDocuments.data.labels = [];

                    data.DiciplineTypes.forEach(function (element) {
                        DiciplineTypes.push(element);
                    });

                    data.DiciplineValues.forEach(function (element) {
                        DiciplineValues.push(element);
                    });

                    for (var Index = 0; Index < DiciplineTypes.length; Index++) {
                        AllDocuments.data.labels.push(DiciplineTypes[Index]);
                    };

                    AllDocuments.data.datasets[0].data = [];
                    data.DiciplineValues.forEach(function (element) {
                        AllDocuments.data.datasets[0].data.push(element);
                    });

                    // re-render the chart
                    AllDocuments.update();
                }
            });
        }

        $('#AllDocumentsProjectID').on('change', function () {
            AllDocumentsFunction($('#AllDocumentsProjectID').val(), $('#AllDocumentsYear').val(), $('#AllDocumentsMonth').val());
        });

        $('#AllDocumentsYear').on('change', function () {
            AllDocumentsFunction($('#AllDocumentsProjectID').val(), $('#AllDocumentsYear').val(), $('#AllDocumentsMonth').val());
        });

        $('#AllDocumentsMonth').on('change', function () {
            AllDocumentsFunction($('#AllDocumentsProjectID').val(), $('#AllDocumentsYear').val(), $('#AllDocumentsMonth').val());
        });

