        });

        $('#DiciplineProduceProcessMonth').on('change', function () {
            DiciplineProduceProcessFunction($('#DiciplineProduceProcessProjectID').val(), $('#DiciplineProduceProcessDiciplineType').val(), $('#DiciplineProduceProcessYear').val(), $('#DiciplineProduceProcessMonth').val());
        });

