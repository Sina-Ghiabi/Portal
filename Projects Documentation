    <script>

        $(document).ready(function () {

            AFCDocumentsFunction($('#AFCDocumentsProjectID').val(), $('#AFCDocumentsYear').val());

            AllDocumentsFunction($('#AllDocumentsProjectID').val(), $('#AllDocumentsYear').val(), $('#AllDocumentsMonth').val());

            AverageAFCDocumentsFunction($('#AverageAFCDocumentsProjectID').val(), $('#AverageAFCDocumentsYear').val());

            AverageToGetAFCFunction($('#AverageToGetAFCProjectID').val(), $('#AverageToGetAFCYear').val());

            DiciplineProduceProcessFunction(($('#DiciplineProduceProcessProjectID').val() != null ? $('#DiciplineProduceProcessProjectID').val() : null, $('#DiciplineProduceProcessDiciplineType').val() != null ? $('#DiciplineProduceProcessDiciplineType').val() : null, $('#DiciplineProduceProcessYear').val() != null ? $('#DiciplineProduceProcessYear').val() : null, $('#DiciplineProduceProcessMonth').val() != null ? $('#DiciplineProduceProcessMonth').val() : null));

            ProjectsAverageDocuments($('#TransmitalDateStart').val(), $('#TransmitalDateEnd').val());

            DocumentsStatus($('#DocumentsStatusTableProjectID').val());

        });

        function ProjectsAverageDocuments(StartDate, EndDate) {
            $.ajax({
                url: '/Dashboard/AverageProjectsDocuments',
                data: { "StartDate": StartDate, "EndDate": EndDate },
                success: function (data) {

                    var ProjectsNames = [];
                    var ProjectsAverageDocuments = [];
                    AverageProjectsDocuments.data.labels = [];

                    data.ProjectsNames.forEach(function (element) {
                        ProjectsNames.push(element);
                    });

                    data.ProjectsAverageDocuments.forEach(function (element) {
                        ProjectsAverageDocuments.push(element);
                    });

                    for (var Index = 0; Index < ProjectsNames.length; Index++) {
                        AverageProjectsDocuments.data.labels.push(ProjectsNames[Index]);
                    };

                    AverageProjectsDocuments.data.datasets[0].data = [];
                    data.ProjectsAverageDocuments.forEach(function (element) {
                        AverageProjectsDocuments.data.datasets[0].data.push(element);
                        AverageProjectsDocuments.data.datasets[0].backgroundColor.push("#" + ((1 << 24) * Math.random() | 0).toString(16));
                    });

                    // re-render the chart
                    AverageProjectsDocuments.update();
                }
            });
        }


        var DefaultStartDate = "";
        var DefaultEndDate = "";

        window.setInterval(function () {
            if ($('#TransmitalDateStart').val() != DefaultStartDate) { ProjectsAverageDocuments($('#TransmitalDateStart').val(), $('#TransmitalDateEnd').val()); DefaultStartDate = $('#TransmitalDateStart').val(); }
            if ($('#TransmitalDateEnd').val() != DefaultEndDate) { ProjectsAverageDocuments($('#TransmitalDateStart').val(), $('#TransmitalDateEnd').val()); DefaultEndDate = $('#TransmitalDateEnd').val(); }
        }, 100);


    </script>

    <script>

        var AverageProjectsDocumentsChart = document.getElementById("AverageProjectsDocuments").getContext('2d');
        var AverageProjectsDocuments = new Chart(AverageProjectsDocumentsChart, {
            type: 'pie',
            data: {
                labels: [],
                datasets: [{
                    backgroundColor: [],
                    data: []
                }]
            }
        });
    </script>






    <script>
        //AFCDocumentsChart

        var AFCDocumentsChart = document.getElementById('AFCDocuments');
        var AFCDocuments = new Chart(AFCDocumentsChart, {
            type: 'bar',
            data: {
                labels: [],
                datasets: [{
                    label: "تعداد",
                    data: [],
                    backgroundColor: 'rgba(0, 175, 155)'
                }]
            },
            options: {
                responsive: true,
                legend: {
                    position: 'bottom',
                    align: 'end',
                    labels: {
                        fontFamily: 'Shabnam',
                        fontColor: 'black'
                    }
                },
                tooltips: {
                    mode: 'label',
                    label: 'mylabel',
                    callbacks: {
                        label: function (tooltipItem, data) {
                            var tooltipValue = data.datasets[tooltipItem.datasetIndex].data[tooltipItem.index];
                            return data.datasets[tooltipItem.datasetIndex].label + " : " + parseInt(tooltipValue).toLocaleString();
                        },
                    },
                    titleFontFamily: "Shabnam",
                    bodyFontFamily: "Shabnam"
                },
                scales: {
                    xAxes: [{
                        stacked: true,
                    }],

                    yAxes: [{
                        ticks: {
                            beginAtZero: true,
                            min: 0,
                        },
                    }]
                }
            }
        });

        function AFCDocumentsFunction(ProjectID, SelectedYear) {
            $.ajax({
                url: '/Dashboard/AFCDocuments',
                data: { "ProjectID": ProjectID, "Year": SelectedYear },
                success: function (data) {
                    var DiciplineTypes = [];
                    var DiciplineValues = [];
                    AFCDocuments.data.labels = [];

                    data.DiciplineTypes.forEach(function (element) {
                        DiciplineTypes.push(element);
                    });

                    data.DiciplineValues.forEach(function (element) {
                        DiciplineValues.push(element);
                    });

                    for (var Index = 0; Index < DiciplineTypes.length; Index++) {
                        AFCDocuments.data.labels.push(DiciplineTypes[Index]);
                    };

                    AFCDocuments.data.datasets[0].data = [];
                    data.DiciplineValues.forEach(function (element) {
                        AFCDocuments.data.datasets[0].data.push(element);
                    });

                    // re-render the chart
                    AFCDocuments.update();
                }
            });
        }

        $('#AFCDocumentsProjectID').on('change', function () {
            AFCDocumentsFunction($('#AFCDocumentsProjectID').val(), $('#AFCDocumentsYear').val());
        });

        $('#AFCDocumentsYear').on('change', function () {
            AFCDocumentsFunction($('#AFCDocumentsProjectID').val(), $('#AFCDocumentsYear').val());
        });

    </script>

