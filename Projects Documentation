        function DiciplineProduceProcessFunction(ProjectID, Dicipline, SelectedYear, SelectedMonth) {
            $.ajax({
                url: '/Dashboard/DiciplineProduceProcess',
                data: { "ProjectID": ProjectID, "Dicipline": Dicipline, "StartYear": SelectedYear, "StartMonth": SelectedMonth },
                success: function (data) {
                    var ChartTitles = [];
                    var ChartValues = [];
                    DiciplineProduceProcess.data.labels = [];

                    data.ChartTitles.forEach(function (element) {
                        ChartTitles.push(element);
                    });

                    data.ChartValues.forEach(function (element) {
                        ChartValues.push(element);
                    });

                    for (var Index = 0; Index < ChartTitles.length; Index++) {
                        DiciplineProduceProcess.data.labels.push(ChartTitles[Index]);
                    };

                    DiciplineProduceProcess.data.datasets[0].data = [];
                    data.ChartValues.forEach(function (element) {
                        DiciplineProduceProcess.data.datasets[0].data.push(element);
                    });

                    // re-render the chart
                    DiciplineProduceProcess.update();
                }
            });
        }

        $('#DiciplineProduceProcessProjectID').on('change', function () {
            DiciplineProduceProcessFunction($('#DiciplineProduceProcessProjectID').val(), $('#DiciplineProduceProcessDiciplineType').val(), $('#DiciplineProduceProcessYear').val(), $('#DiciplineProduceProcessMonth').val());
        });

        $('#DiciplineProduceProcessDiciplineType').on('change', function () {
            DiciplineProduceProcessFunction($('#DiciplineProduceProcessProjectID').val(), $('#DiciplineProduceProcessDiciplineType').val(), $('#DiciplineProduceProcessYear').val(), $('#DiciplineProduceProcessMonth').val());
        });

        $('#DiciplineProduceProcessYear').on('change', function () {
